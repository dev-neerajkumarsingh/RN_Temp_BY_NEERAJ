{
  "name": "RNTemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android-clean": "cd android && ./gradlew clean && cd .. && react-native run-android",
    "android-release": "cd android && ./gradlew assembleProdRelease",
    "ios": "react-native run-ios",
    "ios-clean": "cd ios && rm -rf Pods build Podfile.lock && pod install && cd .. && react-native run-ios",
    "lint": "eslint .",
    "start": "watchman watch-del-all && react-native start --reset-cache",
    "deep-clean": "rm -rf node_modules package-lock.json && watchman watch-del-all && rm -rf ios/build && rm -rf android/build android/app/build android/app/.cxx && npm i",
    "test": "jest",
    "prepare": "husky",
    "rename": "npx react-native-rename"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@d11/react-native-fast-image": "^8.12.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-navigation/native": "^7.1.19",
    "@react-navigation/stack": "^7.6.4",
    "@reduxjs/toolkit": "^2.10.1",
    "@tanstack/react-query": "^5.90.10",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/file-loader": "^5.0.4",
    "axios": "^1.11.0",
    "buffer": "^6.0.3",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-config": "1.5.9",
    "react-native-edge-to-edge": "^1.7.0",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-image-crop-picker": "^0.51.1",
    "react-native-keyboard-controller": "^1.19.5",
    "react-native-keychain": "^10.0.0",
    "react-native-quick-base64": "^2.2.2",
    "react-native-quick-crypto": "^0.7.17",
    "react-native-reanimated": "^4.1.5",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.16.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^15.12.1",
    "react-native-worklets": "^0.7.0",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.81.5",
    "@react-native/eslint-config": "0.81.5",
    "@react-native/metro-config": "0.81.5",
    "@react-native/typescript-config": "0.81.5",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.19.0",
    "husky": "^9.1.7",
    "jest": "^29.6.3",
    "lint-staged": "^16.2.7",
    "prettier": "2.8.8",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-rename": "^3.2.17",
    "react-test-renderer": "19.1.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
